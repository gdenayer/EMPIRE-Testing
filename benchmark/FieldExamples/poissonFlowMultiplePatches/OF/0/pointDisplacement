FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    top1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    bottom1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    top2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    bottom2
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    top3
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    bottom3
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    frontback
    {
    	type            empty;
    }
}

