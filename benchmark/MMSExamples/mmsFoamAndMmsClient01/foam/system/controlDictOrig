FoamFile
{
    version         2.0;
    format          ascii;

    root            "/home/fisch/OpenFOAM/fisch-1.3/run/tutorials/icoFoam";
    case            "cavity";
    instance        "system";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs ( "libgroovyBC.so" ) ;


application     myPisoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         TotalTimeDummy;

deltaT          TimeStepDummy;

writeControl    timeStep;

writeInterval   OutputTimestepDummy;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression uncompressed;

timeFormat      general;

timePrecision   8;

graphFormat     raw;

runTimeModifiable yes;

adjustTimeStep  no;

maxCo           0.2;

